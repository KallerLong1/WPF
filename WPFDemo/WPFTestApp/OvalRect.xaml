<UserControl x:Class="WpfTestApp.OvalRect"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfTestApp"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Rectangle x:Name="rec" RadiusX="10" RadiusY="10" Width="55" Height="25" StrokeThickness="1" Stroke="Black" HorizontalAlignment="Center"
                   VerticalAlignment="Center" Fill="White"/>
        <TextBlock Text="Start" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
